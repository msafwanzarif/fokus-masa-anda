{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Fokus Masa Anda!",
  "version": "0.1.0",
  "identifier": "com.fokus-masa-anda.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Fokus! Manfaatkan Masa Anda.",
        "width": 1200,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
